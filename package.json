{
  "name": "mittr-irving",
  "version": "1.0.0",
  "description": "MIT Technology Review frontend application based on Irving, a headless app for Wordpress.",
  "main": "main.js",
  "scripts": {
    "preinstall": "if [ -z $VIP_GO_APP_ID ]; then npx check-node-version --package; fi",
    "preflight": "npx @automattic/vip-go-preflight-checks",
    "build": "if [ -z $VIP_GO_APP_ID ]; then npm run webpack; else exit 0; fi",
    "webpack": "webpack --config config/webpack.config.js --mode production",
    "dev": "NODE_ENV=development node nodemon.js",
    "start": "NODE_ENV=production node server/index.js",
    "postinstall": "npm run build",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --config=./.eslintrc.json ./",
    "test:watch": "NODE_ENV=test jest --watch",
    "styleguide:dev": "NODE_ENV=development styleguidist server --config ./config/styleguide.config.js",
    "styleguide:build": "NODE_ENV=production styleguidist build --config ./config/styleguide.config.js && surge -d mit-technology-review.surge.sh -p ./styleguide"
  },
  "repository": {
    "type": "git",
