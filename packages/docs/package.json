{
    "name": "@irvingjs/docs",
    "private": true,
    "description": "Documentation for Irving JS packages, components, and usage",
    "homepage": "https://github.com/alleyinteractive/irving/docs",
    "repository": {
      "type": "git",
      "url": "git@github.com:alleyinteractive/irving.git"
    },
    "license": "MIT",
    "author": "Alley (Alley.co)",
    "main": "main.js",
    "scripts": {
      "storybook": "start-storybook -s ./stories/public -p 6006",
      "storybook:build": "build-storybook -s ./stories/public",
      "storybook:deploy": "surge storybook-static --domain storybook.irvingjs.com",
      "storybook:release": "npm run storybook:build && npm run storybook:deploy",
      "storybook-qa:deploy": "surge storybook-static --domain storybook-qa.irvingjs.com",
      "storybook-qa:release": "npm run storybook:build && npm run storybook-qa:deploy"
    },
    "devDependencies": {
      "@storybook/addon-a11y": "^6.3.0-alpha.15",
      "@storybook/addon-actions": "^6.3.0-alpha.15",
      "@storybook/addon-docs": "^6.3.0-alpha.15",
      "@storybook/addon-info": "^6.0.0-alpha.2",
      "@storybook/addon-links": "^6.3.0-alpha.15",
      "@storybook/addons": "^6.3.0-alpha.15",
      "@storybook/builder-webpack5": "~6.3.0-alpha.15",
      "@storybook/react": "^6.3.0-alpha.15"
    },
    "engines": {
      "node": "12",
      "npm": "6"
    },
  }
