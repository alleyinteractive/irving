<!doctype html>
<html lang="en" <%- head.htmlAttributes.toString() %>>
<head>
    <%- head.start.toString() %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%- head.title.toString() %>
    <%- head.meta.toString() %>
    <%- head.link.toString() %>
    <%- head.base.toString() %>
    <%- head.style.toString() %>

    <%# additional head tags %>
    <%- head.end.toString() %>
</head>
<body <%- head.bodyAttributes.toString() %>>
    <%- head.noscript.toString() %>
    <%- appHtml %>
    <% if (errorToDisplay) { %>
        <pre style="background-color: white; padding: 20px; white-space: pre-wrap;">
            <%= errorToDisplay %>
        </pre>
    <% } %>
</body>
</html>
