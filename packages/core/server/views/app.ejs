<!doctype html>
<html lang="en" <%- head.htmlAttributes %> >
<head>
    <%- head.start %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%- head.title %>
    <%- head.meta %>
    <%- head.link %>
    <%- head.base %>
    <%- head.style %>

    <%# Runtime accessible environment variables %>
    <script>
      window.__ENV__ = <%- env %>;
    </script>

    <%- head.script %>

    <%# pre-rendered serialized Redux state %>
    <script defer id="preload">
      window.__PRELOADED_STATE__ = <%- preRenderedState %>;
    </script>

    <%# webpack runtime, main app bundle, initial chunks, and any other tags user adds %>
    <%- head.end %>
</head>
<body <%- head.bodyAttributes %>>
  <noscript>
      You need to enable JavaScript to view this site.
  </noscript>
  <%- head.noscript %>
  <div id="root"><%- appHtml %></div>
</body>
</html>
