<!doctype html>
<html lang="en" <%- head.htmlAttributes.toString() %> >
<head>
    <%- head.start.toString() %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%- head.title.toString() %>
    <%- head.meta.toString() %>
    <%- head.base.toString() %>

    <%# Runtime accessible environment variables %>
    <script>
      window.__ENV__ = <%- env %>;
    </script>

    <%- head.link.toString() %>
    <%- head.script.toString() %>
    <%- head.style.toString() %>

    <%# pre-rendered serialized Redux state %>
    <script defer id="preload">
      window.__PRELOADED_STATE__ = <%- preRenderedState %>;
    </script>

    <%# webpack runtime, main app bundle, initial chunks, and any other tags user adds %>
    <%- head.end.toString() %>
</head>
<body <%- head.bodyAttributes.toString() %>>
  <noscript>
      You need to enable JavaScript to view this site.
  </noscript>
  <%- head.noscript.toString() %>
  <div id="root"><%- appHtml %></div>
</body>
</html>
